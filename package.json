{
  "name": "gc-cli",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint src/. test/.",
    "build": "npm run lint && npm run test && npm run compile",
    "test": "mocha --compilers js:babel/register test/**",
    "test:debug": "node --debug _mocha",
    "precompile": "rimraf dist",
    "compile": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briandipalma/gc-cli.git"
  },
  "dependencies": {
    "capitalize": "^0.5.0",
    "chalk": "^0.5.1",
    "global-compiler": "git://github.com/briandipalma/global-compiler",
    "immutable": "^3.0.3",
    "js-formatter": "git://github.com/briandipalma/js-formatter",
    "minimist": "^1.1.0",
    "recast": "^0.10.0",
    "through2": "^0.6.1",
    "vinyl-fs": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^4.6.3",
    "del": "^1.1.1",
    "eslint": "^0.17.1",
    "mocha": "^2.1.0",
    "rimraf": "^2.3.2"
  },
  "preferGlobal": true,
  "bin": "./bin/gc-cli-cli.js"
}
