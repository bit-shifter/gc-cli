{
  "name": "global-compiler",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/. test/.",
    "build": "npm run lint && npm run test && npm run compile",
    "test": "mocha -R min --compilers js:babel/register test/**",
    "test:debug": "node --debug _mocha",
    "precompile": "rimraf dist",
    "compile": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briandipalma/global-compiler.git"
  },
  "dependencies": {
    "array-includes": "^1.0.5",
    "capitalize": "^0.5.0",
    "immutable": "^3.0.3",
    "recast": "^0.10.0",
    "winston": "^0.9.0"
  },
  "devDependencies": {
    "babel": "^4.6.1",
    "eslint": "^0.17.0",
    "mocha": "^2.1.0",
    "rimraf": "^2.3.2"
  }
}
